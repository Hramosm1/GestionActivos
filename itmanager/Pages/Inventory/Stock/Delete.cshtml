@page
@model itmanager.Pages.Inventory.Details.DeleteModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>DinDetalleInventario</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Fecha
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.DinFecha)
        </dd>
        <dt class="col-sm-2">
            Estado
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.DinEstado)
        </dd>
        <dt class="col-sm-2">
            Usuario
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.DinUsuario)
        </dd>
        <dt class="col-sm-2">
            Comentarios
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.DinComentarios)
        </dd>
        <dt class="col-sm-2">
           Activo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.Act.ActId)
        </dd>
        <dt class="col-sm-2">
           Identificador
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.Inv.InvId)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="DinDetalleInventario.DinId" />
        <input type="submit" value="Borrar" class="btn btn-danger" /> |
        <a asp-page="./Index">Volver a la lista</a>
    </form>
</div>
</body>
</html>
