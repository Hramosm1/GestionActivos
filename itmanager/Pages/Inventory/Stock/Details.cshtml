@page
@model itmanager.Pages.Inventory.Details.DetailsModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>

<div>
    <h4>DinDetalleInventario</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Fecha
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.DinFecha)
        </dd>
        <dt class="col-sm-2">
           Estado
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.DinEstado)
        </dd>
        <dt class="col-sm-2">
            Usuario
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.DinUsuario)
        </dd>
        <dt class="col-sm-2">
            Comentarios
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.DinComentarios)
        </dd>
        <dt class="col-sm-2">
            Activo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.Act.ActId)
        </dd>
        <dt class="col-sm-2">
            Identificador
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DinDetalleInventario.Inv.InvId)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.DinDetalleInventario.DinId">Editar</a> |
    <a asp-page="./Index">Volver a la lista</a>
</div>
</body>
</html>
