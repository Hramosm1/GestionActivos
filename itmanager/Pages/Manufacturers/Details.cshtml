@page "{id:int}"
@model itmanager.Pages.Manufacturers.DetailsModel

@{
    ViewData["Module"] = "Fabricantes";
    ViewData["Title"] = "Detalle de Fabricantes";
}

<partial name="_Breadcrumb"/>

<div>
    <h4>Fabricante ID: @Html.DisplayFor(model => model.TfaTipoFabricante.TfaId)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
           Nombre
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TfaTipoFabricante.TfaNombre)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-page="./Edit" asp-route-id="@Model.TfaTipoFabricante.TfaId">Editar</a> |
    <a class="btn btn-warning" asp-page="./Index">Volver a la lista</a>
</div>
<br />
