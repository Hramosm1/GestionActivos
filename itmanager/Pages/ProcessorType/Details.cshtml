@page "{id:int}"
@model itmanager.Pages.ProcessorType.DetailsModel

@{
    ViewData["Module"] = "Tipo de Procesador";
    ViewData["Title"] = "Edición de Tipos de Procesador";
}

<partial name="_Breadcrumb"/>

<div>
    <h4>Tipo de Procesador ID: @Html.DisplayFor(model => model.TprTipoProcesador.TprId)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
           Nombre
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TprTipoProcesador.TprNombre)
        </dd>
        <dt class="col-sm-2">
           Imagen
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TprTipoProcesador.TprImagen)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-page="./Edit" asp-route-id="@Model.TprTipoProcesador.TprId">Editar</a> |
    <a class="btn btn-warning" asp-page="./Index">Volver a la lista</a>
</div>
<br />
