@page "{id:int}"
@model itmanager.Pages.Parameters.DetailsModel

@{
    ViewData["Module"] = "Parametros";
    ViewData["Title"] = "Detalles de Parametro";
}

<partial name="_Breadcrumb"/>

<div>
    <h4>Parametro ID: @Html.DisplayFor(model => model.ParParametro.ParId)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nombre
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ParParametro.ParNombre)
        </dd>
        <dt class="col-sm-2">
            Valor #1
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ParParametro.ParValor1)
        </dd>
        <dt class="col-sm-2">
           Valor #2
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ParParametro.ParValor2)
        </dd>
        <dt class="col-sm-2">
            Tipo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ParParametro.ParTipoNavigation.TipId)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-page="./Edit" asp-route-id="@Model.ParParametro.ParId">Editar</a> |
    <a class="btn btn-warning" asp-page="./Index">Volver a la lista</a>
</div>
<br />
