<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewData["Title"] - itmanager</title>

    <script src="~/js/font-awesome-912497cdbf.js" crossorigin="anonymous"></script>
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/animate.css" rel="stylesheet">
    <link href="~/css/style.css" rel="stylesheet">
</head>

<body class="gray-bg" style="background-image: url('img/back.jpg');">

    <div class="middle-box text-center loginscreen animated fadeInDown">
        <div>
            <div>
                <h1 class="logo-name">TI+</h1>

            </div>
            <h2>Bienvenido a IT+ Manager</h2>
            <p><h3>Administración de activos para el área de TI.</h3></p>
            <p>Login in.</p>
            
            @RenderBody()

            <p class="m-t"> <small>Boostrap 5 Framework. 2022</small> </p>
        </div>
    </div>

    

    <!-- Mainly scripts -->
    <script src="~/js/jquery-3.6.0.js"></script>
    <script src="~/js/bootstrap.bundle.min.js"></script>

</body>

</html>
