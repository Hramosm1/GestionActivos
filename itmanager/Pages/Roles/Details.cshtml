@page "{id:int}"
@model itmanager.Pages.Roles.DetailsModel

@{
    ViewData["Module"] = "Roles";
    ViewData["Title"] = "Detalle de Rol";
}

<partial name="_Breadcrumb"/>

<div>
    <h4>Rol ID: @Html.DisplayNameFor(model => model.RolRole.RolId)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
           Nombre
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RolRole.RolNombre)
        </dd>
        <dt class="col-sm-2">
           Descripción
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RolRole.RolDescripcion)
        </dd>
        <dt class="col-sm-2">
           Bloqueado
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RolRole.RolBloqueado)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-page="./Edit" asp-route-id="@Model.RolRole.RolId">Editar</a> |
    <a class="btn btn-warning" asp-page="./Index">Volver a la lista</a>
</div>
